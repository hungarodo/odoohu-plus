<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- HU Invoice Maintenance cron -->
    <record id="hu_invoice_maintenance_cron"
            model="ir.cron"
    >
        <field name="name">HU+ Invoice Maintenance</field>
        <field name="code">model.run_invoice_maintenance_cron()</field>
        <field name="doall"
               eval="False"
        />
        <field name="interval_number">8</field>
        <field name="interval_type">minutes</field>
        <field name="model_id"
               ref="l10n_hu_invoice.model_l10n_hu_invoice"
        />
        <field name="numbercall">-1</field>
        <field name="state">code</field>
    </record>
</odoo>

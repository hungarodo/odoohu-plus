<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Window action -->
    <record id="account_move_line_vat_action"
            model="ir.actions.act_window"
    >
        <field name="name">Account Move Lines</field>
        <field name="context">{'journal_type':'general', 'search_default_posted':1, 'search_default_group_by_tax_tags': 1}</field>
        <field name="domain">[('display_type', 'not in', ('line_section', 'line_note')), ('parent_state', '!=', 'cancel')]</field>
        <field name="res_model">account.move.line</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">tree,pivot,graph,kanban</field>
    </record>
    <!-- Window action list view -->
    <record id="account_move_line_vat_action_list"
            model="ir.actions.act_window.view"
    >
        <field name="act_window_id"
               ref="account_move_line_vat_action"
        />
        <field name="sequence">8</field>
        <field name="view_id"
               ref="account.view_move_line_tree"
        />
        <field name="view_mode">tree</field>
    </record>
    <!-- Window action pivot view -->
    <record id="account_move_line_vat_action_pivot"
            model="ir.actions.act_window.view"
    >
        <field name="act_window_id"
               ref="account_move_line_vat_action"
        />
        <field name="sequence">16</field>
        <field name="view_id"
               ref="account.view_move_line_pivot"
        />
        <field name="view_mode">pivot</field>
    </record>
    <!-- Window action graph view -->
    <record id="account_move_line_vat_action_graph"
            model="ir.actions.act_window.view"
    >
        <field name="act_window_id"
               ref="account_move_line_vat_action"
        />
        <field name="sequence">24</field>
        <field name="view_id"
               ref="account.account_move_line_graph_date"
        />
        <field name="view_mode">graph</field>
    </record>
    <!-- Window action kanban view -->
    <record id="account_move_line_vat_action_kanban"
            model="ir.actions.act_window.view"
    >
        <field name="act_window_id"
               ref="account_move_line_vat_action"
        />
        <field name="sequence">32</field>
        <field name="view_id"
               ref="account.account_move_line_view_kanban"
        />
        <field name="view_mode">kanban</field>
    </record>
</odoo>

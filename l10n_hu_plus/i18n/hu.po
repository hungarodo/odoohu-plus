#. module: l10n_hu_plus
#: model:mail.template,body_html:l10n_hu_plus.account_move_proforma_mail_template
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        <p style=\"font-weight:bold;\">\n"
"                            Dear <t t-out=\"object.partner_id.name\"></t>!\n"
"                        </p>\n"
"                        <p>\n"
"                            Please find attached our proforma document.\n"
"                        </p>\n"
"                        <t t-if=\"object.l10n_hu_proforma_name\">\n"
"                            <p>\n"
"                                <strong>Reference: <t t-out=\"object.l10n_hu_proforma_name\"></t></strong>\n"
"                            </p>\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <strong>Reference: Proforma</strong>\n"
"                        </t>\n"
"                        <p>\n"
"                            <strong>Amount: <t t-out=\"format_amount(object.amount_total,object.currency_id)\"></t></strong>\n"
"                        </p>\n"
"                        <t t-if=\"object.invoice_origin\">\n"
"                            <p>\n"
"                                <strong>Origin: <t t-out=\"object.invoice_origin\"></t></strong>\n"
"                            </p>\n"
"                        </t>\n"
"                        <t t-if=\"object.ref\">\n"
"                            <p>\n"
"                                <strong>Client Reference: <t t-out=\"object.ref\"></t></strong>\n"
"                            </p>\n"
"                        </t>\n"
"                        <p>\n"
"                            Please do not hesitate to contact us if you have any questions.\n"
"                        </p>\n"
"                        <p>\n"
"                            Best Regards,\n"
"                        </p>\n"
"                        <p>\n"
"                            <t t-out=\"user.signature or ''\"></t>\n"
"                        </p>\n"
"                        <br>\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        <p style=\"font-weight:bold;\">\n"
"                            Kedves <t t-out=\"object.partner_id.name\"></t>!\n"
"                        </p>\n"
"                        <p>\n"
"                            Mellékelve küldjük díjbekérőnket.\n"
"                        </p>\n"
"                        <t t-if=\"object.l10n_hu_proforma_name\">\n"
"                            <p>\n"
"                                <strong>Hivatkozás: <t t-out=\"object.l10n_hu_proforma_name\"></t></strong>\n"
"                            </p>\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <strong>Hivatkozás: Proforma</strong>\n"
"                        </t>\n"
"                        <p>\n"
"                            <strong>Összeg: <t t-out=\"format_amount(object.amount_total,object.currency_id)\"></t></strong>\n"
"                        </p>\n"
"                        <t t-if=\"object.invoice_origin\">\n"
"                            <p>\n"
"                                <strong>Forrás:</strong><t t-out=\"object.invoice_origin\"></t>\n"
"                            </p>\n"
"                        </t>\n"
"                        <t t-if=\"object.ref\">\n"
"                            <p>\n"
"                                <strong>Ügyfél hivatkozás: <t t-out=\"object.ref\"></t></strong>\n"
"                            </p>\n"
"                        </t>\n"
"                        <p>\n"
"                            Kérdés esetén kérjük vegye fel velünk a kapcsolatot.\n"
"                        </p>\n"
"                        <p>\n"
"                            Üdvözlettel,\n"
"                        </p>\n"
"                        <p>\n"
"                            <t t-out=\"user.signature or ''\"></t>\n"
"                        </p>\n"
"                        <br>\n"
"                    </p>\n"
"                </div>\n"
"            "

